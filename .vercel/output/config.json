{
	"version": 3,
	"routes": [
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^\\/_image$",
			"dest": "render"
		},
		{
			"src": "^\\/admin\\/?$",
			"dest": "render"
		},
		{
			"src": "^\\/api\\/admin-login\\/?$",
			"dest": "render"
		},
		{
			"src": "^\\/api\\/categories\\/([^/]+?)\\/?$",
			"dest": "render"
		},
		{
			"src": "^\\/api\\/categories\\/?$",
			"dest": "render"
		},
		{
			"src": "^\\/api\\/init-db\\/?$",
			"dest": "render"
		},
		{
			"src": "^\\/api\\/products\\/([^/]+?)\\/?$",
			"dest": "render"
		},
		{
			"src": "^\\/api\\/products\\/?$",
			"dest": "render"
		},
		{
			"src": "^\\/api\\/upload\\/?$",
			"dest": "render"
		},
		{
			"src": "^\\/api\\/whatsapp\\/?$",
			"dest": "render"
		},
		{
			"src": "^\\/$",
			"dest": "render"
		}
	]
}